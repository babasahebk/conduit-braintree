<?xml version="1.0" encoding="UTF-8"?>
<credit-card>
  <token>24sss2</token>
  <cardholder-name><%= cardholder_name %></cardholder-name>
  <expiration-month><%= expiration_month %></expiration-month>
  <expiration-year><%= expiration_year %></expiration-year>
  <card-type>Visa</card-type>
  <bin><%= mock.bin %></bin>
  <last-4><%= mock.last_4 %></last-4>
  <billing-address>
    <street-address><%= billing_address[:street_address] %></street-address>
    <postal-code><%= billing_address[:postal_code] %></postal-code>
  </billing-address>
</credit-card>
